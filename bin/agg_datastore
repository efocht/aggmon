#!/usr/bin/env python2
import os
import sys

if os.path.isdir("../src/aggmon"):
    sys.path.append(os.path.abspath(
        os.path.join(
            os.path.dirname(
                os.path.realpath(__file__)), "..", "src")))

if __name__ == "__main__":
    from aggmon.data_store import aggmon_data_store
    aggmon_data_store(sys.argv[1:])
